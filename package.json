{
  "name": "playground",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "lint": "eslint . && npm run lint:fix",
    "lint:fix": "eslint --fix",
    "build": "vite build",
    "build:lib": "vite build --mode lib && npm run build:types",
    "build:types": "vue-tsc -p tsconfig.build.json && api-extractor run && rimraf temp",
    "release:lib": "bumpp -c \"release: v%s\" packages/package.json && esno scripts/copy.ts && cd output-lib && npm publish",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "up": "taze major -I"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@egoist/tailwindcss-icons": "^1.7.4",
    "@iconify-json/carbon": "^1.1.30",
    "@iconify-json/lucide": "^1.1.180",
    "@iconify/json": "^2.2.199",
    "@microsoft/api-extractor": "^7.43.1",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.9.0",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "eslint": "^9.0.0",
    "eslint-plugin-format": "^0.1.0",
    "esno": "^4.7.0",
    "fs": "0.0.1-security",
    "https-localhost": "^4.7.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "lucide-vue-next": "^0.367.0",
    "pnpm": "^8.15.4",
    "postcss": "^8.4.38",
    "radix-vue": "^1.7.0",
    "rimraf": "^5.0.5",
    "shadcn-vue": "^0.10.3",
    "shiki": "^1.3.0",
    "simple-git": "^3.23.0",
    "simple-git-hooks": "^2.11.1",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "taze": "^0.13.3",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.8.6",
    "vite": "^5.2.0",
    "vite-plugin-lib-inject-css": "^2.0.1",
    "vite-ssg": "^0.23.6",
    "vite-ssg-sitemap": "^0.6.1",
    "vitest": "^1.3.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-sonner": "^1.1.2",
    "vue-tsc": "^2.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
